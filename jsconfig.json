{
  "compilerOptions": {
    "target": "esnext",
    "baseUrl": "./",
    "paths": {
      "@h/*": [
        "src/html/*"
      ],
      "@o/*": [
        "src/html/other/*"
      ],
      "@c/*": [
        "src/html/components/*"
      ],
      "@ui/*": [
        "src/html/UI/*"
      ],
      "@j/*": [
        "/src/js/*"
      ],
      "@s/*": [
        "/src/scss/*"
      ],
      "@i/*": [
        "/src/assets/img/*"
      ],
      "@v/*": [
        "/src/assets/video/*"
      ],
      "@f/*": [
        "/src/assets/files/*"
      ]
    }
  }
}