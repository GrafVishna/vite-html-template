[class*="_icon-"] {
  font-family: 'icons' !important;
  font-style: normal;
  font-weight: normal;
  font-variant: normal;
  text-transform: none;
  line-height: 1;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}


:root {
--_icon-E-mail: "\ea01";
--_icon-Information: "\ea02";
--_icon-Phone: "\ea03";
--_icon-Region: "\ea04";
--_icon-facebook: "\ea05";
--_icon-instagram: "\ea06";
--_icon-star: "\ea07";
}
._icon-E-mail::before { content: var(--_icon-E-mail); }
._icon-Information::before { content: var(--_icon-Information); }
._icon-Phone::before { content: var(--_icon-Phone); }
._icon-Region::before { content: var(--_icon-Region); }
._icon-facebook::before { content: var(--_icon-facebook); }
._icon-instagram::before { content: var(--_icon-instagram); }
._icon-star::before { content: var(--_icon-star); }


$_icon-E-mail = '\ea01'
$_icon-Information = '\ea02'
$_icon-Phone = '\ea03'
$_icon-Region = '\ea04'
$_icon-facebook = '\ea05'
$_icon-instagram = '\ea06'
$_icon-star = '\ea07'
